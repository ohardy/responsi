@import '../global'

$icon-base
  font-family FontAwesome
  font-weight normal
  font-style normal
  text-decoration inherit
  -webkit-font-smoothing antialiased

$icon-base-le
  *margin-{$default-float} .3em
  padding-{$default-float} .6rem

$icon-base-te
  *margin-{$default-opposite} .3em
  padding-{$default-opposite} .6rem

responsi-icon-base($where, $name, $font-size = false, $line-height = false)
  &:{$where}
    @extend $icon-base
    @extend $icon-base-le
    // @extend $icon-{$name}
    content responsi-get-icon-content($name)

    if $font-size
      font-size $font-size
    else
      font-size inherit

    if $line-height
      line-height $line-height
    else
      line-height inherit

icon-le($name, $font-size = false, $line-height = false)
  responsi-icon-base($leading-edge, $name, $font-size, $line-height)

icon-te($name, $font-size = false, $line-height = false)
  responsi-icon-base($trailing-edge, $name, $font-size, $line-height)

icon-before($name, $font-size = false, $line-height = false)
  responsi-icon-base('before', $name, $font-size, $line-height)

icon-after($name, $font-size = false, $line-height = false)
  responsi-icon-base('after', $name, $font-size, $line-height)

